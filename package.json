{
  "name": "json-minifier",
  "version": "0.0.3",
  "description": "A two-way JSON minifier to reduce JSON size and amount of data transferred on clients. Can also act as an obfuscator.",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "lint": "./node_modules/.bin/eslint .",
    "cover": "CONSOLE_LOGGING=false ./node_modules/.bin/nyc --reporter=lcov ./node_modules/.bin/_mocha tests --recursive --timeout=10000 --exit",
    "test": "npm run lint && npm run cover"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dial-once/node-json-minifier.git"
  },
  "author": "Dial Once",
  "license": "GPL-2.0",
  "bugs": {
    "url": "https://github.com/dial-once/node-json-minifier/issues"
  },
  "homepage": "https://github.com/dial-once/node-json-minifier#readme",
  "dependencies": {
    "chai": "^4.2.0",
    "lodash": "^4.17.10"
  },
  "devDependencies": {
    "eslint": "^5.4.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.1",
    "nyc": "^13.1.0",
    "mocha": "^5.2.0",
    "jshint": "^2.9.5",
    "jasmine-node": "^1.14.5"
  }
}
